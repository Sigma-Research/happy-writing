/**app.wxss**/
button {
  margin: 0;
  padding: 0;
  background-color: #ffffff;
  border-radius: 0;
  border: none;
}

button::after {
  border: none;
}

page {
  color: #1A1918;
  background: #FAFAFA;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
page .container {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
page .container .section {
  display: flex;
  flex-direction: column;
}
page .container .section .section-title {
  font-size: 40rpx;
  font-weight: bold;
  margin: 48rpx 0 16rpx 0;
}
page .container .section .section-subtitle {
  color: #666564;
  font-size: 28rpx;
  font-weight: normal;
}
page .container .section .section-content {
  width: 100%;
  display: flex;
  flex-direction: column;
}

