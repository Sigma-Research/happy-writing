/**app.wxss**/
page {
  background: #fafafa;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
page .container {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
page .container .section {
  display: flex;
  flex-direction: column;
}
page .container .section .section-title {
  font-size: 40rpx;
  font-weight: bold;
  margin: 48rpx 0 16rpx 0;
}
page .container .section .section-title .section-subtitle {
  display: inline;
  font-size: 14px;
  font-weight: normal;
}
page .container .section .section-content {
  width: 100%;
  display: flex;
  flex-direction: column;
}

